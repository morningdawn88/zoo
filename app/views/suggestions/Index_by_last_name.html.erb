<table border="1">
 <% @suggestions.each do |ab| %>
    <tr>
      <td><%= ab.first_name %></td>
      <td><%= ab.last_name %></td>
      <td><%= ab.username %></td>
      <td><%= ab.division %></td>
      <td><%= ab.department %></td>
      <td><%= ab.memo %></td>
      <td><%= ab.created_at %></td>
      <td><%= ab.updated_at %></td>
     </tr>
   <% end %>
</table>
<h1>Select a link below to change parameter for viewing<h1>
<%= link_to "Login", :controller => "pages", :action => "login"%></br>
<%= link_to "Modification Date", :controller => "suggestions", :action => "index_by_modified" %></br>
<%= link_to "Department", :controller => "suggestions", :action => "index_by_department" %></br>
<%= link_to "Division", :controller => "suggestions", :action => "index_by_division" %></br>
<%= link_to "Created Date", :controller => "suggestions", :action => "index_by_created" %></br>
